<Application x:Class="TrackTracker.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TrackTracker"
             xmlns:vm="clr-namespace:TrackTracker.GUI.ViewModels"
             StartupUri="GUI/Views/MainWindow.xaml">
    <Application.Resources>
        <vm:DataViewModel x:Key="DataViewModel"/>
        <vm:PlayzoneViewModel x:Key="PlayzoneViewModel"/>
        <vm:StatisticsViewModel x:Key="StatisticsViewModel"/>
        <vm:TracklistViewModel x:Key="TracklistViewModel"/>
        <vm:CommonControlsViewModel x:Key="CommonControlsViewModel"/>
    </Application.Resources>
</Application>
